
#include <iostream>

using namespace	std;

int main(){

	int A[4][5];

	// cin >> A[1] >> A[2] >> A[3] ...

	// Message to the user
	 cout << "input numbers : ";
	// user inputs for the for loop
	for (int i = 0; i < 10; i++){
		cin >> A[i];
	} 

	// output the values in the array
	for (int i = 0; i < 10; i++){
		cout << A[i] << " ";
	}

}